{
  "name": "org.plunderludics.plundertools",
  "version": "0.0.3",
  "displayName": "Plunderludics.Plundertools",
  "description": "tools for making games with emulated games using unity-hawk",
  "dependencies": {
    "com.unity.shadergraph": "14.0.9",
    "com.unity.nuget.newtonsoft-json": "3.0.2",
    "com.dbrizov.naughtyattributes": "2.1.4",
    "org.plunderludics.unityhawk": "0.0.13"
  },
  "documentationUrl": "https://github.com/plunderludics/plundertools",
  "keywords": [
    "plunderludics",
    "emulation",
    "bizhawk"
  ],
  "author": {
    "name": "plunderludics working group",
    "url": "https://plunderludics.github.io"
  },
  "scopedRegistries": [
    {
      "name": "package.openupm.com",
      "url": "https://package.openupm.com",
      "scopes": [
        "com.dbrizov.naughtyattributes",
        "org.plunderludics.unityhawk",
        "com.openupm"
      ]
    }
  ],
  "samples": [
    {
      "displayName": "Mixer",
      "description": "Demo scene for using the mixer",
      "path": "Samples~/Mixer"
    }
  ]
